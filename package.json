{
  "name": "maimemo-sdk",
  "version": "3.4.1.0",
  "description": "墨墨背单词API的Node.js SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
